{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["tests/**/*.test.ts"],
  "project": ["{src,tests}/**/*.ts", "!src/**/*.test.ts", "!.github/**/*.yml"],
  "ignoreDependencies": [],
  "ignoreBinaries": ["tsc"],
  "ignoreExportsUsedInFile": true,
  "github-actions": true,
  "release-it": {
    "config": [".release-it.{json,jsonc,js,cjs,ts,yml,yaml,toml}", "package.json"]
  },
  "rules": {
    "dependencies": "error",
    "devDependencies": "error",
    "exports": "error"
  }
}
